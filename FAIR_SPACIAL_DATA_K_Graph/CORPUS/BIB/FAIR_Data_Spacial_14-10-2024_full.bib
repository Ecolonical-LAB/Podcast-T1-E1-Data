@article{ WOS:000278159300003,
Author = {Sudol, M. and Espi, M. C. Mora and Becheva, E. and Boucher, J. and
   Hennino, T. and Kunne, R. and Marchand, D. and Ong, S. and Ramstein, B.
   and Van de Wiele, J. and Zerguerras, T. and Maas, F. and Kopf, B. and
   Pelizaeus, M. and Steinke, M. and Zhong, J. and Tomasi-Gustafsson, E.},
Title = {Feasibility studies of the time-like proton electromagnetic form factor
   measurements with (P)over-barANDA at FAIR},
Journal = {EUROPEAN PHYSICAL JOURNAL A},
Year = {2010},
Volume = {44},
Number = {3},
Pages = {373-384},
Month = {JUN},
Abstract = {The possibility of measuring the proton electromagnetic form factors in
   the time-like region at FAIR with the (P) over bar ANDA detector is
   discussed. Detailed simulations on signal efficiency for the
   annihilation of p + p into a lepton pair as well as for the most
   important background channels have been performed. It is shown that
   precise measurements of the differential cross-section of the reaction
   (p) over bar + p -> e(-) + e(+) can be obtained in a wide kinematical
   range. The determination of the ratio R of the moduli of the electric
   and magnetic proton form factors will be possible up to a value of
   momentum transfer squared of q(2) similar or equal to 14 (GeV/c)(2) with
   absolute precision from 0.01 to 0.5 (for R similar to 1). The total (p)
   over bar + p -> e(-) + e(+) cross-section will be measured up to q(2)
   similar or equal to 28 (GeV/c)(2). The results obtained from simulated
   events are compared to the existing data. Sensitivity to the two-photon
   exchange mechanism is also investigated.},
