@article{ WOS:000329758500005,
Author = {Benardini, III, James N. and La Duc, Myron T. and Ballou, David and
   Koukol, Robert},
Title = {Implementing Planetary Protection on the Atlas V Fairing and Ground
   Systems Used to Launch the Mars Science Laboratory},
Journal = {ASTROBIOLOGY},
