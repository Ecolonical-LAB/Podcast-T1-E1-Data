@article{ WOS:000984275000014,
Author = {David, Romain and Ohmann, Christian and Boiten, Jan-Willem and Abadia,
   Monica Cano and Bietrix, Florence and Canham, Steve and Chiusano, Maria
   Luisa and Dastru, Walter and Laroquette, Arnaud and Longo, Dario and
   Mayrhofer, Michaela Th. and Panagiotopoulou, Maria and Richard, Audrey
   S. and Goryanin, Sergey and Verde, Pablo Emilio},
Title = {An iterative and interdisciplinary categorisation process towards FAIRer
   digital resources for sensitive life-sciences data},
Journal = {SCIENTIFIC REPORTS},
Year = {2022},
Volume = {12},
Number = {1},
Month = {DEC 5},
Abstract = {For life science infrastructures, sensitive data generate an additional
   layer of complexity. Cross-domain categorisation and discovery of
   digital resources related to sensitive data presents major
   interoperability challenges. To support this FAIRification process, a
   toolbox demonstrator aiming at support for discovery of digital objects
   related to sensitive data (e.g., regulations, guidelines, best practice,
   tools) has been developed. The toolbox is based upon a categorisation
   system developed and harmonised across a cluster of 6 life science
   research infrastructures. Three different versions were built, tested by
   subsequent pilot studies, finally leading to a system with 7 main
   categories (sensitive data type, resource type, research field, data
   type, stage in data sharing life cycle, geographical scope, specific
   topics). 109 resources attached with the tags in pilot study 3 were used
   as the initial content for the toolbox demonstrator, a software tool
   allowing searching of digital objects linked to sensitive data with
   filtering based upon the categorisation system. Important next steps are
   a broad evaluation of the usability and user-friendliness of the
   toolbox, extension to more resources, broader adoption by different
   life-science communities, and a long-term vision for maintenance and
   sustainability.},
