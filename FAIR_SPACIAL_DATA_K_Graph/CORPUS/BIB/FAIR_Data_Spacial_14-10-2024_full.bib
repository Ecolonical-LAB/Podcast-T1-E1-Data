@article{ WOS:000473734200020,
Author = {Nandan, Shyam and Ouillon, Guy and Sornette, Didier and Wiemer, Stefan},
Title = {Forecasting the Full Distribution of Earthquake Numbers Is Fair, Robust,
   and Better},
Journal = {SEISMOLOGICAL RESEARCH LETTERS},
Year = {2019},
