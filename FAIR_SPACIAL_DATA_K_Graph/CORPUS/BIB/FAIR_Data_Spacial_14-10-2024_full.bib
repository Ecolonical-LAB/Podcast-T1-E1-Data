@article{ WOS:001059937200001,
Author = {Zhang, Guoqiang and Dai, Yueyue and Wu, Jian and Zhu, Xiaojie and Lu,
   Yunlong},
Title = {Swarm Learning-based Secure and Fair Model Sharing for Metaverse
   Healthcare},
