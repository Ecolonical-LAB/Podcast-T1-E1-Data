@article{ WOS:000167630200001,
Author = {Griffiths, D},
Title = {The side-fairing elements of Maskit's fundamental domain for the modular
   group in genus two},
Journal = {ANNALES ACADEMIAE SCIENTIARUM FENNICAE-MATHEMATICA},
Year = {2001},
Volume = {26},
Number = {1},
Pages = {3-50},
Abstract = {In this paper we study the hyperbolic geometry on a genus 2 surface. The
   main object of study is a subset of the set of hyperbolic lengths of
   closed geodesics on such a surface which arises from an algorithmic
   choice of shortest loops. Maskit has shown that this data can be used to
   identify finite sided polyhedral fundamental set for the modular group
   on the marked hyperbolic surface structures of a given genus. The
   special nature of genus 2 has made it more accessible than in higher
   genus and we are able to produce a more detailed picture of the domain
   and its side-pairing transformations. If the domain can be shown to
   satisfy certain basic topological criteria, according to a classical
   theorem of Poincare, then this would give a set of geometrical
   generators and relations for the modular group.},
