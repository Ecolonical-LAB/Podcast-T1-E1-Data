@article{ WOS:000312725500027,
Author = {Campbell-Washburn, Adrienne E. and Price, Anthony N. and Wells, Jack A.
   and Thomas, David L. and Ordidge, Roger J. and Lythgoe, Mark F.},
Title = {Cardiac arterial spin labeling using segmented ECG-gated Look-Locker
   FAIR: Variability and repeatability in preclinical studies},
Journal = {MAGNETIC RESONANCE IN MEDICINE},
