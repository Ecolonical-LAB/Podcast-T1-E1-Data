@article{ WOS:000304324500020,
Author = {van Herpen, Erica and van Nierop, Erjen and Sloot, Laurens},
Title = {The relationship between in-store marketing and observed sales for
   organic versus fair trade products},
Journal = {MARKETING LETTERS},
Year = {2012},
Volume = {23},
