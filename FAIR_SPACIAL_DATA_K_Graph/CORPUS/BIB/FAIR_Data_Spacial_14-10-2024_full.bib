@article{ WOS:000785237800007,
Author = {Lu, Lu and Meng, Xuhui and Cai, Shengze and Mao, Zhiping and Goswami,
   Somdatta and Zhang, Zhongqiang and Karniadakis, George Em},
Title = {A comprehensive and fair comparison of two neural operators (with
   practical extensions) based on FAIR data},
