@article{ WOS:000242913800019,
Author = {Tappe, A. and Rho, J. and Reach, W. T.},
Title = {Shock processing of interstellar dust and polycyclic aromatic
   hydrocarbons in the supernova remnant N132D},
Journal = {ASTROPHYSICAL JOURNAL},
Year = {2006},
