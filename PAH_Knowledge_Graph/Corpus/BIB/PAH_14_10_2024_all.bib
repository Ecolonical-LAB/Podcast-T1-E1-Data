@article{ WOS:000253454000021,
Author = {Galliano, Frederic and Dwek, Eli and Chanial, Pierre},
Title = {Stellar evolutionary effects on the abundances of polycyclic aromatic
   hydrocarbons and supernova-condensed dust in galaxies},
