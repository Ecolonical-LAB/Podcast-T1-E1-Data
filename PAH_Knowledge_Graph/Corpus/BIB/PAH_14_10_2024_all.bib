@article{ WOS:000286662000067,
Author = {Ricca, Alessandra and Bauschlicher, Jr., Charles W. and Allamandola,
   Louis J.},
Title = {PROTONATED POLYCYCLIC AROMATIC HYDROCARBONS REVISITED},
Journal = {ASTROPHYSICAL JOURNAL},
Year = {2011},
Volume = {727},
Number = {2},
Month = {FEB 1},
Abstract = {We reconsider the contribution that singly protonated polycyclic
   aromatic hydrocarbons (PAHs; HPAH(+)s) might make to the Class A
   component of the 6.2 mu m interstellar emission feature in light of the
   recent experimental measurements of protonated naphthalene and coronene.
   Our calculations on the small HPAH(+)s have a band near 6.2 mu m, as
   found in experiment. While the larger HPAH(+)s still have emission near
   6.2 mu m, the much larger intensity of the band near 6.3 mu m overwhelms
   the weaker band at 6.2 mu m, so that the 6.2 mu m band is barely
   visible. Since the large PAHs are more representative of those in the
   interstellar medium, our work suggests that large HPAH(+)s cannot be
   major contributors to the observed emission at 6.2 mu m (i.e., Class A
   species). Saturating large PAH cations with hydrogen atoms retains the
   6.2 mu m Class A band position, but the rest of the spectrum is
   inconsistent with observed spectra.},
