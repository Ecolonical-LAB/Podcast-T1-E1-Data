@article{ WOS:000341302200010,
Author = {Candian, A. and Sarre, P. J. and Tielens, A. G. G. M.},
Title = {POLYCYCLIC AROMATIC HYDROCARBONS WITH ARMCHAIR EDGES AND THE 12.7 μm
   BAND},
Journal = {ASTROPHYSICAL JOURNAL LETTERS},
Year = {2014},
Volume = {791},
Number = {1},
Month = {AUG 10},
Abstract = {In this Letter, we report the results of density functional theory
   calculations on medium-sized neutral polycyclic aromatic hydrocarbon
   (PAH) molecules with armchair edges. These PAH molecules possess strong
   C-H stretching and bending modes around 3 mu m and in the fingerprint
   region (10-15 mu m), and also strong ring deformation modes around 12.7
   mu m. Perusal of the entries in the NASA Ames PAHs Database shows that
   ring deformation modes of PAHs are common, although generally weak.
   Therefore, we propose that armchair PAHs with N-C > 65 are responsible
   for the 12.7 mu m aromatic infrared band in Hii regions and discuss the
   astrophysical implications in the context of the PAH life cycle.},
