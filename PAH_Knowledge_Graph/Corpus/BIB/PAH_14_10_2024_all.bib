@article{ WOS:000426709000004,
Author = {Shannon, M. J. and Peeters, E. and Cami, J. and Blommaert, J. A. D. L.},
Title = {Polycyclic Aromatic Hydrocarbon Emission Toward the Galactic Bulge},
